  "scripts": {
    "build": "tsc --project .",
    "clean": "rimraf dist build",
    "webpack": "webpack --mode=development",
    "webpackp": "webpack --mode=production",
    "test": "npm run test:node",
    "test:node": "npm run clean && npm run build && mocha src/tests/",
    "zz": " webpack --mode=development --config webpack-test-node.config.js && mocha build/test",
    "test:browser": "npm run clean && npm run build && webpack --mode=development --config webpack-test-browser.config.js",
    "testbundle": "mocha dist/test-bundle-node.js",
    "lint": "tslint --project .",
    "validate": "npm-run-all --parallel test lint",
    "precommit": "npm run validate",
    "prepush": "npm run validate"
  },
