{
  "name": "soi_battleship",
  "version": "0.0.1",
  "description": "BattleShip for SOI",
  "main": "dest/battleship.js",
  "scripts": {
    "2": "npm run webpack && http-server dist -o -c-1",
    "build:css": "node-sass-chokidar src/ -o src/ && tcm src --camelCase",
    "build:ts": "tsc --skipLibCheck --project .",
    "build": "npm run clean && npm run build:css && npm run build:ts",
    "clean": "rimraf dist build src/**/*.{js,js.map} src/app/style/*.{css,css.d.ts}",
    "webpack": "npm run build && webpack --mode=development",
    "webpackp": "npm run build && webpack --mode=production",
    "test": "npm run test:node --silent -s",
    "test:node": "npm run build && mocha src/tests/**/*.js",
    "test:browser": "npm run build && webpack --mode=development --config webpack-test-browser.config.js && http-server dist -o -c-1",
    "test:browser2": "npm run build && webpack --mode=development --config webpack-test-browser.config.js",
    "test:node2": "npm run build && webpack --mode=development --config webpack-test-node.config.js",
    "testbundle": "mocha dist/test-bundle-node.js",
    "lint": "tslint --project . && tsfmt --verify",
    "lint:fix": "tslint --project . --fix && tsfmt -r",
    "validate": "npm-run-all --parallel test:node lint",
    "precommit": "npm run validate",
    "prepush": "npm run validate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://GeekAthelon:Git8035Hub@github.com/GeekAthelon/soi_battleship.git"
  },
  "author": "Geek Athelon",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/GeekAthelon/soi_battleship/issues"
  },
  "homepage": "https://github.com/GeekAthelon/soi_battleship#readme",
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.5",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "husky": "^1.1.3",
    "mini-css-extract-plugin": "^0.4.4",
    "mocha": "^5.2.0",
    "node-sass": "^4.10.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "typed-css-modules": "^0.3.7",
    "typescript": "^3.1.6",
    "typescript-formatter": "^7.2.2",
    "typings-for-css-modules-loader": "^1.7.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "@types/chai": "^4.1.7",
    "firebase": "^5.5.8",
    "svgjs": "^2.6.2",
    "sweetalert": "^2.1.2",
    "webpack-md5-hash": "0.0.6"
  }
}
